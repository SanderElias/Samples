{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    // "strict": true,
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "ESNext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2017",
    "allowJs": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2020",
      "dom"
    ],

    "paths": {
      "@se-ng/observable-hooks": [
        "dist/se-ng/observable-hooks"
      ],
      "@se-ng/observable-hooks/*": [
        "dist/se-ng/observable-hooks/*"
      ],
      "@se-ng/swapi": [
        "dist/se-ng/swapi"
      ],
      "@se-ng/swapi/*": [
        "dist/se-ng/swapi/*"
      ],
      "@se-ng/observable-utils": [
        "dist/se-ng/observable-utils"
      ],
      "@se-ng/observable-utils/*": [
        "dist/se-ng/observable-utils/*"
      ],
      "@se-ng/let": [
        "dist/se-ng/let"
      ],
      "@se-ng/let/*": [
        "dist/se-ng/let/*"
      ]
    }
  }
}
