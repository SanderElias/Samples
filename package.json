{
  "name": "samples",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=16.14.2",
    "yarn": "1.22.*"
  },
  "type": "module",
  "scripts": {
    "build.samples": "wireit",
    "build.deploy": "wireit",
    "build.lib": "wireit",
    "build.lib.let": "wireit",
    "build.lib.observable-hooks": "wireit",
    "build.lib.observable-utils": "wireit",
    "build.lib.swapi": "wireit",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "scully": "scully",
    "scully:serve": "scully serve --404=index",
    "server.dev": "cd projects/folderCrud && tsc-watch --onSuccess 'node --experimental-vm-modules ../../dist/folderCrud'",
    "serveHtml": "npx light-server -s ./ -w '*' --port 8080 ",
    "start": "ng serve --port 4301 --ssl",
    "test": "ng test"
  },
  "wireit": {
    "build.deploy": {
      "command": "scully --project samples",
      "dependencies": [
        "build.samples"
      ]
    },
    "build.samples": {
      "command": "ng build --configuration production",
      "files": [
        "src/*",
        "tsconfig.app.json"
      ],
      "output": [
        "dist/samples"
      ],
      "dependencies": [
        "build.lib"
      ]
    },
    "build.lib": {
      "dependencies": [
        "build.lib.observable-hooks",
        "build.lib.swapi",
        "build.lib.observable-utils",
        "build.lib.let"
      ]
    },
    "build.lib.let": {
      "command": "ng build @se-ng/let --configuration production",
      "files": [
        "projects/se-ng/let/src/*",
        "projects/se-ng/let/tsconfig.lib.json"
      ]
    },
    "build.lib.observable-hooks": {
      "command": "ng build @se-ng/observable-hooks --configuration production",
      "files": [
        "projects/se-ng/observable-hooks/src/*",
        "projects/se-ng/observable-hooks/tsconfig.lib.json"
      ]
    },
    "build.lib.observable-utils": {
      "command": "ng build @se-ng/observable-utils --configuration production",
      "files": [
        "projects/se-ng/observable-utils/src/*",
        "projects/se-ng/observable-utils/tsconfig.lib.json"
      ]
    },
    "build.lib.swapi": {
      "command": "ng build @se-ng/swapi --configuration",
      "files": [
        "projects/se-ng/swapi/src/*",
        "projects/se-ng/swapi/tsconfig.lib.json"
      ]
    }
  },
  "resolutions": {
    "asciidoctor.js": "1.5.9",
    "scully/**/asciidoctor.js": "1.5.9"
  },
  "dependencies": {
    "@angular/animations": "15.2.0-next.3",
    "@angular/common": "15.2.0-next.3",
    "@angular/compiler": "15.2.0-next.3",
    "@angular/core": "15.2.0-next.3",
    "@angular/elements": "^15.2.0-next.3",
    "@angular/forms": "15.2.0-next.3",
    "@angular/platform-browser": "15.2.0-next.3",
    "@angular/platform-browser-dynamic": "15.2.0-next.3",
    "@angular/router": "15.2.0-next.3",
    "@faker-js/faker": "^7.6.0",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@npm/types": "^1.0.2",
    "@scullyio/ng-lib": "^3.0.0-beta.0",
    "@scullyio/scully": "^3.0.0-beta.0",
    "@scullyio/scully-plugin-puppeteer": "^3.0.0-beta.0",
    "@toast-ui/editor": "^3.2.1",
    "@types/js-yaml": "^4.0.1",
    "@types/jsdom": "^20.0.1",
    "@types/msgpack-lite": "^0.1.7",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@validointi/core": "^0.0.0-beta.4",
    "animate-css-grid": "^1.4.3",
    "asciidoctor": "^2.2.6",
    "bson": "^4.7.0",
    "chance": "^1.1.9",
    "core-js": "^3.26.1",
    "devcert": "^1.2.2",
    "document-register-element": "^1.7.2",
    "front-matter": "^4.0.2",
    "guess-parser": "^0.4.22",
    "idb-keyval": "^6.2.0",
    "messagepack": "^1.1.8",
    "msgpack-lite": "^0.1.26",
    "open-props": "^1.5.1",
    "path-to-regexp": "^6.2.1",
    "replicator": "^1.0.3",
    "rxjs": "^7.6.0",
    "tslib": "^2.4.1",
    "wireit": "0.9.2-pre.1",
    "yaml": "^2.1.3",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0-next.3",
    "@angular-eslint/builder": "15.1.0",
    "@angular-eslint/eslint-plugin": "15.1.0",
    "@angular-eslint/eslint-plugin-template": "15.1.0",
    "@angular-eslint/schematics": "15.1.0",
    "@angular-eslint/template-parser": "15.1.0",
    "@angular/cli": "15.2.0-next.3",
    "@angular/compiler-cli": "15.2.0-next.3",
    "@angular/language-service": "15.2.0-next.3",
    "@playwright/test": "^1.28.1",
    "@types/chance": "^1.1.3",
    "@types/node": "^18.11.10",
    "@types/yargs": "^17.0.15",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.28.0",
    "ng-packagr": "^15.0.1",
    "prettier": "^2.8.0",
    "sharp": "^0.31.2",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "4.8.4",
    "yargs": "^17.6.2"
  }
}
