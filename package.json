{
  "name": "samples",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "npm": ">=10",
    "yarn": "This project is switched to NPM",
    "node": ">=18.14.2"
  },
  "type": "module",
  "scripts": {
    "build.samples": "wireit",
    "build.deploy": "wireit",
    "build.lib": "wireit",
    "build.lib.let": "wireit",
    "build.lib.observable-hooks": "wireit",
    "build.lib.observable-utils": "wireit",
    "build.lib.swapi": "wireit",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "scully": "scully",
    "scully:serve": "scully serve --404=index",
    "server.dev": "cd projects/folderCrud && tsc-watch --onSuccess 'node --experimental-vm-modules ../../dist/folderCrud'",
    "serveHtml": "npx light-server -s ./ -w '*' --port 8080 ",
    "start": "ng serve --port 4301 --open",
    "test": "ng test"
  },
  "wireit": {
    "build.deploy": {
      "command": "node node_modules/@scullyio/scully/bin/scully.mjs --project samples",
      "dependencies": [
        "build.samples"
      ]
    },
    "build.samples": {
      "command": "ng build --configuration production",
      "files": [
        "src/*",
        "tsconfig.app.json"
      ],
      "output": [
        "dist/samples"
      ],
      "dependencies": [
        "build.lib"
      ]
    },
    "build.lib": {
      "dependencies": [
        "build.lib.observable-hooks",
        "build.lib.swapi",
        "build.lib.observable-utils",
        "build.lib.let"
      ]
    },
    "build.lib.let": {
      "command": "ng build @se-ng/let --configuration production",
      "files": [
        "projects/se-ng/let/src/*",
        "projects/se-ng/let/tsconfig.lib.json"
      ]
    },
    "build.lib.observable-hooks": {
      "command": "ng build @se-ng/observable-hooks --configuration production",
      "files": [
        "projects/se-ng/observable-hooks/src/*",
        "projects/se-ng/observable-hooks/tsconfig.lib.json"
      ]
    },
    "build.lib.observable-utils": {
      "command": "ng build @se-ng/observable-utils --configuration production",
      "files": [
        "projects/se-ng/observable-utils/src/*",
        "projects/se-ng/observable-utils/tsconfig.lib.json"
      ]
    },
    "build.lib.swapi": {
      "command": "ng build @se-ng/swapi --configuration",
      "files": [
        "projects/se-ng/swapi/src/*",
        "projects/se-ng/swapi/tsconfig.lib.json"
      ]
    }
  },
  "dependencies": {
    "@angular/animations": "18.0.5",
    "@angular/common": "18.0.5",
    "@angular/compiler": "18.0.5",
    "@angular/core": "18.0.5",
    "@angular/elements": "^18.0.5",
    "@angular/forms": "18.0.5",
    "@angular/platform-browser": "18.0.5",
    "@angular/platform-browser-dynamic": "18.0.5",
    "@angular/router": "18.0.5",
    "@faker-js/faker": "^8.4.1",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@ngneat/falso": "^7.2.0",
    "@npm/types": "^1.0.2",
    "@toast-ui/editor": "^3.2.2",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.6",
    "@types/msgpack-lite": "^0.1.11",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@validointi/core": "^1.6.0",
    "animate-css-grid": "^1.5.1",
    "bson": "^6.7.0",
    "chance": "^1.1.11",
    "colorjs.io": "^0.5.0",
    "core-js": "^3.37.1",
    "devcert": "^1.2.2",
    "document-register-element": "^1.7.2",
    "front-matter": "^4.0.2",
    "guess-parser": "^0.4.22",
    "idb-keyval": "^6.2.1",
    "messagepack": "^1.1.12",
    "msgpack-lite": "^0.1.26",
    "open-props": "^1.7.4",
    "path-to-regexp": "^6.2.2",
    "replicator": "^1.0.5",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "typescipt": "^1.0.0",
    "yaml": "^2.4.2"
  },
  "overrides": {
    "asciidoctor.js": {}
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.6",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/cli": "18.0.6",
    "@angular/compiler-cli": "18.0.5",
    "@angular/language-service": "18.0.5",
    "@playwright/test": "^1.42.1",
    "@types/chance": "^1.1.6",
    "@types/node": "^20.12.12",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "ng-packagr": "^18.0.0",
    "prettier": "^3.2.5",
    "sharp": "^0.33.4",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "5.4.5",
    "wireit": "^0.14.4",
    "yargs": "^17.7.2"
  }
}
