{
  "name": "samples",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "npm": ">=10",
    "yarn": "This project is switched to NPM",
    "node": ">=18.14.2"
  },
  "type": "module",
  "scripts": {
    "build.samples": "wireit",
    "build.deploy": "wireit",
    "build.lib": "wireit",
    "build.lib.let": "wireit",
    "build.lib.observable-hooks": "wireit",
    "build.lib.observable-utils": "wireit",
    "build.lib.swapi": "wireit",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "server.dev": "cd projects/folderCrud && tsc-watch --onSuccess 'node --experimental-vm-modules ../../dist/folderCrud'",
    "serveHtml": "npx light-server -s ./ -w '*' --port 8080 ",
    "start": "wireit",
    "test": "ng test",
    "ntest": "wireit"
  },
  "wireit": {
    "start": {
      "command": "ng serve samples --port 4204",
      "files": [
        "src/**",
        "tsconfig.app.json"
      ],
      "dependencies": [
        "build.lib"
      ]
    },
    "ntest": {
      "command": "node --test  out-tsc/spec/utils/signals/async-computed.spec.js",
      "files": [
        "src/utils/signals/**"
      ],
      "dependencies": [
        "compileTest"
      ]
    },
    "compileTest": {
      "command": "ngc --project src/tsconfig.spec.json",
      "files": [
        "src/signals/**"
      ],
      "dependencies": [
        "build.lib"
      ]
    },
    "build.deploy": {
      "command": "echo 'Deploying to production'",
      "dependencies": [
        "build.samples"
      ]
    },
    "build.samples": {
      "command": "ng build --configuration production",
      "files": [
        "src/**",
        "tsconfig.app.json"
      ],
      "output": [
        "dist/samples"
      ],
      "dependencies": [
        "build.lib"
      ]
    },
    "build.lib": {
      "dependencies": [
        "build.lib.observable-hooks",
        "build.lib.swapi",
        "build.lib.observable-utils",
        "build.lib.let",
        "build.lib.signalUtils"
      ]
    },
    "build.lib.let": {
      "command": "ng build @se-ng/let --configuration production",
      "files": [
        "projects/se-ng/let/src/*",
        "projects/se-ng/let/tsconfig.lib.json"
      ]
    },
    "build.lib.observable-hooks": {
      "command": "ng build @se-ng/observable-hooks --configuration production",
      "files": [
        "projects/se-ng/observable-hooks/src/*",
        "projects/se-ng/observable-hooks/tsconfig.lib.json"
      ]
    },
    "build.lib.observable-utils": {
      "command": "ng build @se-ng/observable-utils --configuration production",
      "files": [
        "projects/se-ng/observable-utils/src/*",
        "projects/se-ng/observable-utils/tsconfig.lib.json"
      ]
    },
    "build.lib.swapi": {
      "command": "ng build @se-ng/swapi --configuration",
      "files": [
        "projects/se-ng/swapi/src/*",
        "projects/se-ng/swapi/tsconfig.lib.json"
      ]
    },
    "build.lib.signalUtils": {
      "command": "ng build @se-ng/signalUtils --configuration production",
      "files": [
        "projects/se-ng/signal-utils/src/*",
        "projects/se-ng/signal-utils/tsconfig.lib.json"
      ]
    }
  },
  "dependencies": {
    "@angular/animations": "19.0.0-next.3",
    "@angular/common": "19.0.0-next.3",
    "@angular/compiler": "19.0.0-next.3",
    "@angular/core": "19.0.0-next.3",
    "@angular/elements": "19.0.0-next.3",
    "@angular/forms": "19.0.0-next.3",
    "@angular/platform-browser": "19.0.0-next.3",
    "@angular/platform-browser-dynamic": "19.0.0-next.3",
    "@angular/platform-server": "19.0.0-next.3",
    "@angular/router": "19.0.0-next.3",
    "@angular/ssr": "19.0.0-next.3",
    "@faker-js/faker": "^8.4.1",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@ngneat/falso": "^7.2.0",
    "@toast-ui/editor": "^3.2.2",
    "animate-css-grid": "^1.5.1",
    "bson": "^6.8.0",
    "chance": "^1.1.12",
    "colorjs.io": "^0.5.2",
    "core-js": "^3.38.1",
    "express": "^4.19.2",
    "front-matter": "^4.0.2",
    "idb-keyval": "^6.2.1",
    "jasmine": "^5.2.0",
    "jsdom": "^25.0.0",
    "messagepack": "^1.1.12",
    "msgpack-lite": "^0.1.26",
    "open-props": "^1.7.5",
    "path-to-regexp": "^7.1.0",
    "replicator": "^1.0.5",
    "rxjs": "^7.8.1",
    "tslib": "^2.7.0",
    "yaml": "^2.5.0"
  },
  "overrides": {
    "asciidoctor.js": {}
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.0.0-next.3",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "19.0.0-next.3",
    "@angular/compiler-cli": "19.0.0-next.3",
    "@angular/language-service": "19.0.0-next.3",
    "@playwright/test": "^1.46.1",
    "@types/chance": "^1.1.6",
    "@types/eslint": "^9.6.1",
    "@types/express": "^4.17.21",
    "@types/jasmine": "^5.1.4",
    "@types/js-yaml": "^4.0.9",
    "@types/jsdom": "^21.1.7",
    "@types/msgpack-lite": "^0.1.11",
    "@types/node": "^22.5.1",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@web/test-runner": "^0.18.3",
    "eslint": "^9.9.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.3",
    "ng-packagr": "^19.0.0-next.1",
    "prettier": "^3.3.3",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "tsx": "^4.19.0",
    "typescript": "5.5.4",
    "wireit": "^0.14.8",
    "yargs": "^17.7.2"
  }
}
